{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
      "ecmaVersion": 5
  },
  "env": {
    "node": false,
    "es6": false,
    "browser": true
  },
  "globals": {},
  "rules": {
    "no-trailing-spaces": 0,
    "semi": [2, "always"],
    "no-eq-null": 2,
    "no-var": 0,
    "no-console": 0
  }
}